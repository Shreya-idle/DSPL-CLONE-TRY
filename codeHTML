        $(document).ready(function () {
            $('img').error(function () {
                $(this).hide();
                $(this).attr('src', '../images/company/default/no_image_front.gif');
                $(this).css("display", "block");
            });

            $(".pagination li>a").text('');
            $('.Nav_inner a').click(function () {
                var div_id = $(this).attr('href');
                $('.Nav_inner a').removeClass("on");
                $(this).addClass("on");
                var temp = $(div_id).offset().top - 170;
                $('html, body').animate({
                    scrollTop: temp
                }, 2000);
            });
            $(document).scroll(function () {
                if ($(document).scrollTop() > 325)
                    $("#nav").delay(1000).addClass("fix");
                else
                    $("#nav").removeClass("fix");
            });

            $('#Events').slides({
                generateNextPrev: true,
                play: 5000,
                pause: 2500,
                hoverPause: true
            });


        });


        $(document).ready(function () {

            //$(window).load(function() {
            //
            //$('img').error(function(){
            //					 $(this).hide();
            //					$(this).attr('src', '../images/company/default/no_image_front.gif');
            //					$(this).css("display","block");
            //				});
            //	});

            $("img").on("error", function () {
                $(this).hide();
                $(this).attr('src', '../images/company/default/no_image_front.gif');
                $(this).css("display", "block");
            });

            if ($(window).width() < 700) {
                $('.Lft_nav li:gt(4)').hide();
            }


            $('[class^="Team_"]').click(function (e) {
                return;//temporary added
                $("span[id^=plsicn_]").removeClass("cross");
                curr_arr = $(this).attr("class").split('_');
                curr_id = curr_arr[1];

                if ($("#desc_pop_" + curr_id).is(':visible')) {
                    $("#desc_pop_" + curr_id).hide();
                    $("#plsicn_" + curr_id).removeClass("cross");
                }
                else {
                    $('[id^="desc_pop_"]').hide();
                    $("#desc_pop_" + curr_id).show();
                    $("#plsicn_" + curr_id).addClass("cross");
                }
            });
            $('.Team_bx Team_').click(function (e) {
                e.preventDefault();
                // hide all span
                var $this = $(this).parent().find('span');
                $(".item span").not($this).hide();
                $this.toggle();
            });
            $('.Nav-tabs > li > a').click(function (event) {
                event.preventDefault();//stop browser to take action for clicked anchor

                //get displaying tab content jQuery selector
                var active_tab_selector = $('.Nav-tabs > li.active > a').attr('href');

                //find actived navigation and remove 'active' css
                var actived_nav = $('.Nav-tabs > li.active');
                actived_nav.removeClass('active');

                //add 'active' css into clicked navigation
                $(this).parents('li').addClass('active');

                //hide displaying tab content
                $(active_tab_selector).removeClass('active');
                $(active_tab_selector).addClass('hide');

                //show target tab content
                var target_tab_selector = $(this).attr('href');
                $(target_tab_selector).removeClass('hide');
                $(target_tab_selector).addClass('active');
            });
            if ($(window).width() <= 900) {
                if ($('#ajxEventImgs').length >= 1) {
                    var detachedDiv = $('#ajxEventImgs').detach();
                    $('.ListImg.active').append(detachedDiv);
                }
            }
        });
        function ajaxloadEventImgs(evtid) {
            if (typeof (evtid) == 'undefined' || evtid == '')
                return false;
            $('.ListImg').removeClass('active');
            $('.ListImg[data-eventID="' + evtid + '"]').addClass('active');
            $.ajax({
                url: "our_company.php?ajxEventId=" + evtid,
                /*beforeSend: function( xhr ) {
                    $("#ajxEventImgs").html("<img src='/images/loading.gif'>");
                  }*/
                success: function (result) {
                    $("#ajxEventImgs").html(result);
                    if ($(window).width() <= 900) {
                        if ($('#ajxEventImgs').length >= 1) {
                            var detachedDiv = $('#ajxEventImgs').detach();
                            $('.ListImg.active').append(detachedDiv);
                        }
                    }
                    $('#dspl_events').slides({
                        generateNextPrev: true,
                        play: 5000,
                        pause: 2500,
                        hoverPause: true
                    });
                    $(".pagination li>a").text('');
                }
            });
        }
    </script>
    <script>
        $(document).ready(function () {

            $(".Benefits li").click(function () {
                var clname = $(this).attr('class');
                $(".BenefitsTxt ul").hide();
                $(".Benefits li span").removeClass('EMParrow');
                $(this).find('span').addClass('EMParrow');
                $(".BenefitsTxt").find('.' + clname).show();
            })
        })

    </script>

